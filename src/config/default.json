{
  "kafka": {
    "datacenters": ["dc1"],
    "options": {
      "groupId": "logs",
      "sessionTimeout": 15000,
      "protocol": ["roundrobin"],
      "fromOffset": "latest",
      "highWaterMark": 4,
      "maxTickMessages": 100
    }
  },
  "db": {
    "unique": "index_logs_on_build_id_and_position"
  }
}
