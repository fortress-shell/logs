{
  "kafka": {
    "datacenters": ["dc1", "dc2"],
    "options": {
      "groupId": "logs",
      "sessionTimeout": 15000,
      "protocol": ["roundrobin"],
      "fromOffset": "latest",
      "highWaterMark": 100,
      "maxTickMessages": 1000
    }
  },
  "db": {
    "unique": "logs_unique"
  }
}
